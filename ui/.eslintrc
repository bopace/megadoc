{
  "env": {
    "browser": true,
    "mocha": true
  },

  "plugins": [
    "react"
  ],

  "globals": {
    "expect": false
  },
  "ecmaFeatures": {
    "blockBindings": true,
    "arrowFunctions": true,
    "classes": true,
    "templateStrings": true,
    "destructuring": true,
    "objectLiteralShorthandMethods": true,
    "spread": true,
    "restParams": true,
    "jsx": true,
  },

  "rules": {
    "dot-notation": 0
  }
}