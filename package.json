{
  "name": "megadoc",
  "version": "6.0.0-beta.1",
  "description": "Fast and sleek and modular documentation generator.",
  "scripts": {
    "build": "NODE_ENV=production packages/megadoc-html-serializer/node_modules/.bin/webpack --config packages/megadoc-html-serializer/webpack.config.js -p --display-chunks",
    "build:dev": "packages/megadoc-html-serializer/node_modules/.bin/webpack --config packages/megadoc-html-serializer/webpack.config.js",
    "coverage": "COVERAGE=1 ./bin/test",
    "doc": "./bin/build-docs",
    "lint": "./bin/lint && ./bin/lint-ui",
    "prepublish": "./bin/lerna-prepublish",
    "start": "./bin/devserver",
    "test": "./bin/test && ./bin/test-ui --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:megadoc/megadoc"
  },
  "keywords": [
    "docs",
    "documentation"
  ],
  "author": "Ahmad Amireh",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/megadoc/megadoc/issues"
  },
  "homepage": "https://github.com/megadoc/megadoc",
  "dependencies": {},
  "devDependencies": {
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "chai": "3.5.0",
    "cheerio": "0.22.0",
    "coveralls": "2.11.4",
    "depcheck": "0.6.7",
    "eslint": "3.19.0",
    "eslint-plugin-mocha": "4.9.0",
    "eslint-plugin-react": "6.10.3",
    "fs-extra": "2.1.2",
    "html": "0.0.10",
    "istanbul": "^1.0.0-alpha.2",
    "lerna": "2.0.0-rc.4",
    "mocha": "2.4.5",
    "mocha-lcov-reporter": "^1.0.0",
    "multiline-slash": "^2.0.0",
    "sinon": "^2.2.0"
  }
}
