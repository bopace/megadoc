{
  "name": "tinydoc",
  "version": "3.2.2",
  "description": "Fast and sleek documentation generator.",
  "main": "index.js",
  "bin": {
    "tinydoc": "./cli/tinydoc"
  },
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test": "npm run-script lint && ./bin/test.sh && ./bin/test-ui.sh --single-run",
