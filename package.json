  },
  "homepage": "https://github.com/tinydoc/tinydoc",
  "dependencies": {
    "babel-core": "5.3.3",
    "babel-loader": "5.0.0",
    "classnames": "^2.1.5",
    "commander": "^2.8.1",
    "css-loader": "0.9.1",
    "deep-get-set": "^0.1.1",
    "dox": "0.7.0",
    "file-loader": "^0.8.4",
    "fs-extra": "0.18.0",
    "git-log-parser": "^1.1.0",
    "glob": "5.0.3",
    "highlight.js": "^8.8.0",
    "htmlparser2": "^3.8.3",
    "imports-loader": "^0.6.4",
