    "start": "./bin/devserver.sh",
    "build": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js -p --display-chunks",
    "docs": "./cli/index.js --config=doc/tinydoc.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:tinydoc/tinydoc"
  },
  "keywords": [
    "docs",
    "documentation"
