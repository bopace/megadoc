{
  "name": "tinydoc",
  "version": "4.0.2",
  "description": "Fast and sleek and modular documentation generator.",
  "main": "lib/index.js",
  "bin": {
    "tinydoc": "./cli/tinydoc",
    "tinydoc-compile": "./cli/tinydoc-compile"
  },
  "scripts": {
    "lint": "eslint lib packages/*/lib",
    "lint:ui": "eslint -c .eslintrc -c .eslintrc--ui ui packages/*/ui",
    "test": "mocha 'lib/**/*.test.js' 'packages/*/lib/**/*.test.js'",
    "test:ui": "karma start --single-run",
    "prepublish": "npm run lint && npm run lint:ui && npm run test && npm run test:ui && npm run build",
    "start": "./bin/devserver.sh",
    "build": "NODE_ENV=production webpack -p --display-chunks",
    "doc": "./cli/tinydoc --config=doc/tinydoc.conf.js",
    "coverage": "istanbul cover --preserve-comments ./node_modules/mocha/bin/_mocha --report lcovonly -- 'lib/**/*.test.js' 'packages/*/lib/**/*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:tinydoc/tinydoc"
  },
  "keywords": [
    "docs",
    "documentation"
  ],
  "author": "Ahmad Amireh",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/tinydoc/tinydoc/issues"
  },
  "homepage": "https://github.com/tinydoc/tinydoc",
  "dependencies": {
    "babel-core": "5.3.3",
    "babel-loader": "5.0.0",
    "classnames": "^2.1.5",
    "commander": "^2.8.1",
    "css-loader": "0.9.1",
    "deep-get-set": "^0.1.1",
    "dom-contains": "^0.2.0",
    "file-loader": "^0.8.4",
    "fs-extra": "0.18.0",
    "fuse.js": "2.2.0",
    "git-log-parser": "^1.1.0",
    "glob": "5.0.3",
    "htmlparser2": "^3.8.3",
    "imports-loader": "^0.6.4",
    "less": "2.3.1",
    "less-loader": "2.0.0",
    "lodash": "3.6.0",
    "marked": "0.3.3",
    "normalize.css": "^3.0.3",
    "raw-loader": "^0.5.1",
    "react": "^0.13.3",
    "style-loader": "0.8.3",
    "url-loader": "0.5.5",
    "webpack": "^1.7.3"
  },
  "devDependencies": {
    "chai": "2.1.0",
    "coveralls": "^2.11.4",
    "eslint": "^1.4.3",
    "eslint-plugin-react": "^3.4.1",
    "highlight.js": "^8.8.0",
    "history": "1.17.0",
    "istanbul": "0.4.3",
    "json-loader": "0.5.1",
    "karma": "^0.13.10",
    "karma-chrome-launcher": "0.1.7",
    "karma-firefox-launcher": "0.1.7",
    "karma-mocha": "0.1.10",
    "karma-sourcemap-loader": "0.3.3",
    "karma-webpack": "^1.7.0",
    "md5": "^2.0.0",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.0.0",
    "multiline-slash": "2.0.0",
    "react-hot-loader": "",
    "react-router": "0.13.x",
    "serve-static": "^1.10.0",
    "sinon": "^1.17.1",
    "tinydoc-plugin-js": "4.x",
    "tinydoc-plugin-markdown": "4.x",
    "webpack-dev-middleware": "1.0.11",
    "webpack-dev-server": "^1.7.0"
  }
}
